<h2>Currently defined APIC-EM policy tags</h2>

<div class="list-group">
  {{#each model as |tag|}}
    {{policy-item item=tag}}
  {{/each}}
</div>

<div class="apps-list">
  <p>app list will appear here</p>

  <span id="ex18-label-1" class="hidden">Example slider label</span>
         <input id="ex19" type="text"
               data-provide="slider"
               data-slider-ticks="[1, 2, 3]"
               data-slider-ticks-labels='["short", "medium", "long"]'
               data-slider-min="1"
               data-slider-max="3"
               data-slider-step="1"
               data-slider-value="3"
               data-slider-tooltip="hide" />
</div>
